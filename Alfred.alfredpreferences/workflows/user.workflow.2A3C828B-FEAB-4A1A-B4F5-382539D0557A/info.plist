				<key>script</key>
				<string>#####################################
# Default to a table if no argument #
#####################################

if [ -z "$1" ]; then
  FLIPPED='┻━┻'
else
  FLIPPED=$(echo $1 | rev | tr 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ!?' 'ɐqɔpǝɟɓɥᴉſʞןɯuodbɹsʇnʌʍxʎzꓯꓭↃꓷƎℲ⅁ΗIᒋꓘ⅂ꟽNOꓒÒꓤSꓕꓵꓥꤵX⅄Z¡¿')
fi

####################################
# Flip both sides or just one side #
####################################

if [ "$both" == "1" ]; then
  echo -n "$FLIPPED $flipper $FLIPPED"
else
  echo -n "$flipper $FLIPPED"
fi</string>
